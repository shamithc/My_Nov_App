<div class="avatar"> 
  
</div>
<%#= fb_authenticate? %>
<%# if authenticated? %>
 <% if fb_authenticate? %>
   <div id="profile-pic">
         <%#= session[:fb_id]%>
         <%#= current_user.try(:identifier) %> 
      <fb:profile-pic uid="<%= session[:fb_id]%>" size="normal" facebook-logo="true"></fb:profile-pic>
   </div>
   <p><%= link_to "Logout", :controller =>'friend',:action=>'destroy_session',:method =>'get' %></p>
<% else %>
  <dl>
    <dt></dt>
    <%= facebook_path %>
    <dd><fb:login-button length="long" onlogin="location.href = '<%= facebook_path %>'" perms="<%= Facebook.config[:perms] %>"></fb:login-button></dd>
    <dd><%#= link_to "Start", new_facebook_path %></dd>
  </dl>
<% end %>

